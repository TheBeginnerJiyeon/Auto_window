-- DML 기본 연습

SELECT * FROM emp;

-- DISTINCT 중복을 제거해서 SELECT 절에서 딱 한번만 쓸 수 있다

SELECT DISTINCT DEPTNO FROM EMP;


SELECT DISTINCT DEPTNO, JOB FROM EMP;
SELECT DISTINCT COMM, JOB FROM EMP;