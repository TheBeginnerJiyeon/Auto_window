<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>BoardWeb</display-name>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>boardEncoding</param-name>
    <param-value>UTF-8</param-value>
  </context-param>
  <servlet>
    <description></description>
    <display-name>GetBoardListServlet</display-name>
    <servlet-name>GetBoardListServlet</servlet-name>
    <servlet-class>com.ssamz.web.board.GetBoardListServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetBoardListServlet</servlet-name>
    <url-pattern>/GetBoardListServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>InsertBoardServlet</display-name>
    <servlet-name>InsertBoardServlet</servlet-name>
    <servlet-class>com.ssamz.web.board.InsertBoardServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InsertBoardServlet</servlet-name>
    <url-pattern>/InsertBoardServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GetBoardServlet</display-name>
    <servlet-name>GetBoardServlet</servlet-name>
    <servlet-class>com.ssamz.web.board.GetBoardServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetBoardServlet</servlet-name>
    <url-pattern>/GetBoardServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>UpdateBoardServlet</display-name>
    <servlet-name>UpdateBoardServlet</servlet-name>
    <servlet-class>com.ssamz.web.board.UpdateBoardServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpdateBoardServlet</servlet-name>
    <url-pattern>/UpdateBoardServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DeleteBoardServlet</display-name>
    <servlet-name>DeleteBoardServlet</servlet-name>
    <servlet-class>com.ssamz.web.board.DeleteBoardServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteBoardServlet</servlet-name>
    <url-pattern>/DeleteBoardServlet</url-pattern>
  </servlet-mapping>
</web-app>